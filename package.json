{
  "name": "diplo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "chokidar": "^3.4.1",
    "handlebars": "^4.7.6",
    "http-proxy-middleware": "^1.0.5",
    "nightmare": "^3.0.2",
    "promise-path": "^1.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "remote-model": "^1.0.2"
  },
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm run dev-react\" \"npm run dev-server\"",
    "dev-react": "react-scripts start",
    "dev-server": "nodemon ./server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "homepage": "http://localhost:49625/diplo",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "sampledata/*",
      "src/*"
    ],
    "delay": "250"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
